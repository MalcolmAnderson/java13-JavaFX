<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.DialogPane?>


<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.DatePicker?>
<DialogPane fx:controller="controller.MainScreenController"
            xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml/1" >
   <headerText >
      Add Modify Part Dialog
   </headerText>
   <content>
      <GridPane vgap="10" hgap="10">
         <Label text="Add Part" GridPane.rowIndex="0" GridPane.columnIndex="0"/>
         <TextField GridPane.rowIndex="0" GridPane.columnIndex="1"/>
         <Label text="Details:" GridPane.rowIndex="1" GridPane.columnIndex="0"/>
         <TextArea GridPane.rowIndex="1" GridPane.columnIndex="1"/>
         <Label text="Deadline:" GridPane.rowIndex="2" GridPane.columnIndex="1"/>
         <DatePicker GridPane.rowIndex="2" GridPane.columnIndex="1"/>
      </GridPane>
   </content>


</DialogPane>
